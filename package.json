{
  "name": "rx-vision-root",
  "private": true,
  "workspaces": [
    "demo-app",
    "chrome-devtools",
    "chrome-devtools/test/client",
    "chrome-devtools/test/host",
    "frontend",
    "api"
  ],
  "scripts": {
    "build:extension": "npm -w frontend run build && npm -w chrome-devtools run build",
    "start:demo-app": "npm -w demo-app start",
    "start:dev": "npm run start:frontend",
    "start:frontend": "npm -w frontend start",
    "start:host": "npm -w chrome-devtools/test/host run dev",
    "start:client": "npm -w chrome-devtools/test/client run dev",
    "start:iframe-test-apps": "npm-run-all --parallel start:host start:client",
    "publish:api": "npm publish -w api --access public",
    "build:api": "npm -w api run build",
    "patch:api": "npm version patch -w api",
    "clean:modules": "rimraf node_modules package-lock.json api/node_modules api/package-lock.json demo-app/node_modules demo-app/package-lock.json chrome-devtools/node_modules chrome-devtools/package-lock.json chrome-devtools/test/client/node_modules chrome-devtools/test/client/package-lock.json chrome-devtools/test/host/node_modules chrome-devtools/test/host/package-lock.json frontend/node_modules frontend/package-lock.json"
  },
  "dependencies": {
    "@angular/common": "^20.3.4",
    "@angular/compiler": "^20.3.4",
    "@angular/core": "^20.3.4",
    "@angular/forms": "^20.3.4",
    "@angular/platform-browser": "^20.3.4",
    "@angular/router": "^20.3.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.5",
    "@angular/cli": "^20.3.5",
    "@angular/compiler-cli": "^20.3.4",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  }
}