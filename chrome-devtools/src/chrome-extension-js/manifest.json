{
  "name": "RxJS Visualizer â€“ RxVision",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": [
    "storage"
  ],
  "optional_host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "inject-into-host.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "devtools_page": "devtools.html",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "logo_48.png",
    "128": "logo_128.png"
  }
}
